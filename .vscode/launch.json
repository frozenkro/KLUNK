{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "BUNK.src.main:app"
            ],
            "jinja": true,
            "justMyCode": true
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Svelte Debug",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/FRUNT/src"
        }
    ]
}